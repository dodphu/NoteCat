<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/addOrEditNoteFragment">

    <fragment
        android:id="@+id/addOrEditNoteFragment"
        android:name="com.example.notecat.fragment.AddOrEditNoteFragment"
        android:label="fragment_add_or_edit_note"
        tools:layout="@layout/fragment_add_or_edit_note" >
        <action
            android:id="@+id/action_addOrEditNoteFragment_to_allNoteFragment"
            app:destination="@id/allNoteFragment" />
    </fragment>
    <fragment
        android:id="@+id/allNoteFragment"
        android:name="com.example.notecat.fragment.AllNoteFragment"
        android:label="fragment_all_note"
        tools:layout="@layout/fragment_all_note" >
        <action
            android:id="@+id/action_allNoteFragment_to_addOrEditNoteFragment"
            app:destination="@id/addOrEditNoteFragment" />
    </fragment>
</navigation>